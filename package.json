{
  "name": "node_boiler-plate",
  "version": "1.0.0",
  "description": "node_boiler-plate",
  "scripts": {
    "start": "npm run build && cross-env NODE_ENV=production nodemon ./bin/www",
    "dev": "cross-env NODE_ENV=production nodemon",
    "build": "npm run clean && npm run build-ts && npm run copy-views",
    "build-ts": "node --max-old-space-size=4096 ./node_modules/typescript/bin/tsc",
    "clean": "rimraf ./dist",
    "copy-views": "copyfiles -u 1 ./src/**/*.ejs ./dist",
    "upgrade": "npm update --save-dev && npm update --save",
    "test": "echo \"Error: no test specified\" && exit 1"
  },
  "keywords": [
    "DS",
    "Node",
    "Js",
    "Express",
    "Mysql",
    "Typescript"
  ],
